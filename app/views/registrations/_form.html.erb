<%= simple_form_for @registration, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :student_id %>
  <%= f.input :section_id %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                registrations_path, :class => 'btn' %>
  </div>
<% end %>
