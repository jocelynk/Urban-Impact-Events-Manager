class StudentAllergy < ActiveRecord::Base
  attr_accessible :allergy_id, :student_id
  
  belongs_to :allergy
  belongs_to :student
  
  #Validations
  validates_numericality_of :allergy, :only_integer => true, :greater_than_or_equal_to => 0
  validates_numericality_of :student, :only_integer => true


end
